<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="https://jakarta.ee/xml/ns/jakartaee"
	xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd"
	id="WebApp_ID" version="6.0">
	<display-name>Aps</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	<servlet>
		<description>Classe para a manipulação entre view e a classe Controller Main</description>
		<display-name>ControllerView</display-name>
		<servlet-name>ControllerView</servlet-name>
		<servlet-class>controller.ControllerView</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ControllerView</servlet-name>
		<url-pattern>/controller</url-pattern>
	</servlet-mapping>
</web-app>